#Ejercicio 1.8
for n in range(1, 13):
    print(n)

#Ejercicio 1.9
c = 0
for i in range(1000):
    ultimo_digito = (i ** 3) % 10
    if ultimo_digito == 7:
        c = c + 1
print(c)

#Ejercicio 2.1
for s in range (6):
  for p in range(s):
    print("s vale", s, "y p vale", p)

#Ejercicio 2.2
for s in range(5):
    for p in range(3):
        print("s vale", s, "y p vale", p)
